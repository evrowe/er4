{
  "name": "er4",
  "version": "1.1.0",
  "description": "The annual-ish rebuild of my personal site for 2016, now with a back-end",
  "main": "index.js",
  "private": true,
  "directories": {
    "doc": "doc"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack -p",
    "start": "NODE_ENV=production node index.js",
    "server:debug": "NODE_ENV=development node --inspect server.js",
    "server:dev": "NODE_ENV=development nodemon index.js --ignore node_modules/ --ignore app/",
    "server:webpack": "NODE_ENV=development node johnny/dev-server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "npm run build",
    "db:start": "docker-compose up -d db",
    "db:stop": "docker-compose stop db",
    "db:console": "docker-compose exec db psql --user postgres",
    "db:create": "./scripts/create-database.js",
    "db:drop": "./scripts/drop-database.js",
    "db:migrate": "./node_modules/.bin/knex migrate:latest",
    "db:seed": "./node_modules/.bin/knex seed:run",
    "db:setup": "npm run db:migrate && npm run db:seed"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/evrowe/er4"
  },
  "engines": {
    "node": "5.11.1"
  },
  "author": "Evan Rowe",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "csswring": "^5.0.0",
    "eslint": "^2.12.0",
    "eslint-plugin-react": "^5.1.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "npm-install-webpack-plugin": "^4.0.1",
    "postcss-color-function": "^2.0.1",
    "postcss-cssnext": "^2.6.0",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "webpack-dashboard": "0.0.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "autotrack": "^1.0.0",
    "babel-polyfill": "^6.9.1",
    "bluebird": "^3.4.1",
    "bookshelf": "^0.10.0",
    "compression": "^1.6.2",
    "cors": "^2.7.1",
    "css-loader": "^0.23.1",
    "dotenv": "^2.0.0",
    "ember": "^1.0.0-pre.2",
    "ember-load-initializers": "^0.5.1",
    "express": "^4.13.4",
    "express-session": "^1.14.0",
    "jquery": "^2.2.4",
    "knex": "^0.11.10",
    "markdown-it": "^7.0.0",
    "milligram": "^1.1.0",
    "moment": "^2.14.1",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-github2": "^0.1.10",
    "pg": "^6.1.0",
    "postcss-loader": "^0.9.1",
    "react": "^15.3.1",
    "react-css-modules": "^3.7.7",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-thunk": "^2.1.0",
    "restify": "^4.1.1",
    "restify-router": "^0.2.1",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  }
}
